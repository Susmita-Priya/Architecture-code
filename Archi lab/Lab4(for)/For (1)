.data
	Comma: .asciiz ","
	Sum: .asciiz "\n Summation of 1 to 10 is: "
.text
main: 
	li $t0, 1
	j for_loop
	
	for_loop: 
		bgt $t0, 10 Exit
	
	#print
	li $v0, 1
	move $a0, $t0
	syscall
	
	#add
	add $t1, $t1, $t0
	#no comma on 10
	beq $t0, 10 Exit
	
	#comma print
	li $v0, 4
	la $a0, Comma
	syscall
	
	#increment
	addi $t0, $t0, 1
	j for_loop
	
		
	Exit:
	
	li $v0, 4
	la $a0, Sum
	syscall
	
	#print sum
	li $v0, 1
	move $a0, $t1
	syscall	
		li $v0, 10
		syscall